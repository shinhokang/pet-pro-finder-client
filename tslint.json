{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*."]
  },
  "rules": {
    "member-access": false,
    "no-unused-expression": true,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "ordered-imports": true
  }
}
